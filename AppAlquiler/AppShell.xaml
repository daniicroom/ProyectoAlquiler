    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AppAlquiler"
    Shell.FlyoutBehavior="Disabled">

    <Tab>
        
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

        <ShellContent
        Title="Inicio Reserva"
        ContentTemplate="{DataTemplate local:fInicioReserva}"
        Route="fInicioResrva" />

        <ShellContent
        Title="Registro Cliente"
        ContentTemplate="{DataTemplate local:fRegistroCliente}"
        Route="fRegistroCliente" />

    </Tab>

    

</Shell>
